RewriteEngine On
RewriteRule ^authentification$ index.php [L]
RewriteRule ^create-account$ /controllers/create_account_controller.php [L]
RewriteRule ^create-account-([a-z]+)$ /controllers/create_account_controller.php?type=$1 [L]
RewriteRule ^introduction$ /controllers/introduction_controller.php [L]
RewriteRule ^news$ /controllers/news_controller.php [L]
RewriteRule ^post-([1-9]+[0-9]*)$ /controllers/post_controller.php?id=$1 [L]
RewriteRule ^dashboard$ /controllers/dashboard_controller.php [L]
RewriteRule ^dashboard-([1-9]+[0-9]*)-([1-9]{4})$ /controllers/dashboard_controller.php?month=$1&year=$2 [L,QSA]
RewriteRule ^profil-([1-9]+[0-9]*)$ /controllers/profil_controller.php?id=$1 [L]
RewriteRule ^my-account$ /controllers/my_account_controller.php [L]
RewriteRule ^members$ /controllers/members_controller.php [L]
RewriteRule ^parameters$ /controllers/parameters_controller.php [L]
RewriteRule ^parameters-([a-z]+)$ /controllers/parameters_controller.php?tab=$1 [L]
RewriteRule ^activation-([1-9]+[0-9]*)-([0-9a-zA-Z]{1,})$ /controllers/activation_controller.php?id=$1&token=$2 [L,QSA]
RewriteRule ^my-company$ /controllers/company_controller.php [L]
ErrorDocument 404 /controllers/404_controller.php